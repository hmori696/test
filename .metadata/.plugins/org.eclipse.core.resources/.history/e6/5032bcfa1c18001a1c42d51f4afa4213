
public class Person {

	private String name = null;
	private int age = 0;


	public Person (String name,int age){
		this.name=name;

		this.age=age;

	}

public String getName(){
	return this.name;
}

public void set(String name){
	this.name = name;
}

public int getAge(){
	return this.age;
}

public void setName(String age){
	this.age = age;
}



}
